{
    "routes": [
        {
            "route": "/admin/*",
            "allowedRoles": [
                "admin"
            ]
        },
        {
            "route": "/authenticated/*",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "auth": {
        "rolesSource": "/api/GetRoles",
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/da532c9a-7613-4e84-99fd-0c807f6d60c8/v2.0",
                    "clientIdSettingName": "AZURE_CLIENT_ID",
                    "clientSecretSettingName": "AZURE_CLIENT_SECRET_APP_SETTING_NAME"
                }
            }
        }
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    }
}
